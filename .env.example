# Required: API base URL (include trailing slash)
EXPO_PUBLIC_API_BASE_URL=https://your-api.example.com/

# Optional: WebSocket notifications URL. If not set, derived from API base (http->ws, https->wss)
EXPO_PUBLIC_WS_NOTIFICATIONS_BASE_URL=wss://your-api.example.com/ws/notifications

# Required: Sentry authentication token
SENTRY_AUTH_TOKEN=sample-sentry-auth-token
SENTRY_DSN=sample-sentry-dsn